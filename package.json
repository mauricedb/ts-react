{
  "name": "ts-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sentry/browser": "5.2.1",
    "bootstrap": "4.3.1",
    "classnames": "2.2.6",
    "formik": "1.5.4",
    "react": "16.8.6",
    "react-bootstrap": "1.0.0-beta.8",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.0",
    "use-abortable-fetch": "2.6.2",
    "use-formik-side-effects": "0.3.0"
  },
  "devDependencies": {
    "@types/classnames": "2.2.7",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.2",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.3",
    "husky": "2.3.0",
    "jest-dom": "3.2.2",
    "prettier": "1.17.0",
    "pretty-quick": "1.10.0",
    "react-scripts": "3.0.1",
    "react-testing-library": "7.0.0",
    "source-map-explorer": "1.8.0",
    "typescript": "3.4.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write {src,public}/**/*.{js,ts,jsx,tsx,css,json,html}",
    "source-map:js": "source-map-explorer ./build/static/js/*.js",
    "source-map:css": "source-map-explorer ./build/static/css/*.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
