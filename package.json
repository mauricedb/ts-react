  "dependencies": {
    "@sentry/browser": "5.15.0",
    "bootstrap": "4.4.1",
    "classnames": "2.2.6",
    "formik": "2.1.4",
    "react": "16.13.1",
    "react-bootstrap": "1.0.0-beta.17",
    "react-dom": "16.13.1",
    "react-router-dom": "5.1.2",
    "use-abortable-fetch": "2.6.11",
    "use-formik-side-effects": "1.1.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^3.0.2",
    "@stryker-mutator/html-reporter": "^3.0.2",
    "@stryker-mutator/jest-runner": "^3.0.2",
    "@stryker-mutator/typescript": "^3.0.2",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1",
    "@types/classnames": "2.2.10",
    "@types/faker": "4.1.10",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.2",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "5.1.3",
    "faker": "4.1.0",
    "husky": "4.2.3",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "react-scripts": "3.4.1",
    "source-map-explorer": "2.4.0",
    "typescript": "3.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "prettier": "prettier --write {src,public}/**/*.{js,ts,jsx,tsx,css,json,html}",
    "source-map:js": "source-map-explorer ./build/static/js/*.js",
    "source-map:css": "source-map-explorer ./build/static/css/*.css",
    "license-checker": "npx license-checker --summary --production --direct"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
