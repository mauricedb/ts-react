    "use-formik-side-effects": "1.1.5"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^4.0.0",
    "@stryker-mutator/html-reporter": "^3.1.0",
    "@stryker-mutator/jest-runner": "^4.0.0",
    "@stryker-mutator/typescript": "^4.0.0",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@types/classnames": "2.2.11",
    "@types/faker": "5.1.6",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.7",
    "cypress": "6.4.0",
    "faker": "5.3.1",
    "husky": "4.3.8",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "react-scripts": "4.0.1",
    "source-map-explorer": "2.5.2",
    "typescript": "4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "cypress:open": "cypress open",
    "prettier": "prettier --write {src,public}/**/*.{js,ts,jsx,tsx,css,json,html}",
    "source-map:js": "source-map-explorer ./build/static/js/*.js",
    "source-map:css": "source-map-explorer ./build/static/css/*.css",
    "license-checker": "npx license-checker --summary --production --direct",
    "dcu": "docker-compose up --build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
