  "dependencies": {
    "@sentry/browser": "5.7.0",
    "bootstrap": "4.3.1",
    "classnames": "2.2.6",
    "formik": "1.5.8",
    "react": "16.10.2",
    "react-bootstrap": "1.0.0-beta.14",
    "react-dom": "16.10.2",
    "react-router-dom": "5.1.2",
    "use-abortable-fetch": "2.6.5",
    "use-formik-side-effects": "1.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.1.2",
    "@testing-library/react": "9.3.0",
    "@types/classnames": "2.2.9",
    "@types/faker": "4.1.6",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.2",
    "@types/react-router-dom": "5.1.0",
    "faker": "4.1.0",
    "husky": "3.0.9",
    "prettier": "1.18.2",
    "pretty-quick": "2.0.0",
    "react-scripts": "3.2.0",
    "source-map-explorer": "2.1.0",
    "typescript": "3.6.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write {src,public}/**/*.{js,ts,jsx,tsx,css,json,html}",
    "source-map:js": "source-map-explorer ./build/static/js/*.js",
    "source-map:css": "source-map-explorer ./build/static/css/*.css",
    "license-checker": "npx license-checker --summary --production --direct"
